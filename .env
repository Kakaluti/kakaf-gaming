# Server Configuration
PORT=3000
NODE_ENV=development
APP_URL=http://localhost:3000

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/kakaf-gaming

# JWT Configuration
JWT_SECRET=your_jwt_secret_here_1234567890
JWT_REFRESH_SECRET=your_refresh_secret_here_1234567890

# Email Configuration (for nodemailer)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password

# Firebase Configuration
FIREBASE_PROJECT_ID=kakaf-gaming
FIREBASE_PRIVATE_KEY=-----BEGIN PRIVATE KEY----------END PRIVATE KEY-----
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-fbsvc@kakaf-gaming.iam.gserviceaccount.com

# Redis Configuration
REDIS_URL=redis://localhost:6379

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://kakaf-gaming.herokuapp.com,https://www.kakaf-gaming.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=logs/app.log
ERROR_LOG_FILE_PATH=logs/error.log

# Security
SESSION_SECRET=generated_session_secret_here
COOKIE_SECRET=generated_cookie_secret_here

# Payment Gateway (if needed)
PAYMENT_GATEWAY_API_KEY=your_payment_gateway_api_key
PAYMENT_GATEWAY_SECRET=your_payment_gateway_secret

# Additional Configuration
ADMIN_URL=http://localhost:3000/admin
CP_ENCRYPTION_KEY=your_cp_encryption_key
CODM_API_BASE_URL=https://api.codm.com
CODM_API_KEY=your_codm_api_key 